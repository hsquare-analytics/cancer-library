<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">

    <changeSet id="20221116134901-1-data" author="jhipster" context="kcure">
        <loadData file="config/liquibase/kcure/fake-data/codebook.csv"
                  separator=";"
                  tableName="ph_codebook"
                  usePreparedStatements="true">
            <column name="id" type="numeric"/>
            <column name="title" type="string"/>
        </loadData>
        <loadData file="config/liquibase/kcure/fake-data/codebook_lookup.csv"
                  separator=";"
                  tableName="ph_codebook_lookup"
                  usePreparedStatements="true">
            <column name="id" type="numeric"/>
            <column name="title" type="string"/>
            <column name="description" type="string"/>
            <column name="order_no" type="number"/>
        </loadData>
    </changeSet>
</databaseChangeLog>
